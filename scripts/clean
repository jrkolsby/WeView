rm ./ch-server/data/data.db
docker container rm -f $(docker ps -a | grep 'ch-client-prod' | awk '{print $1}')
docker container rm -f $(docker ps -a | grep 'ch-server-prod' | awk '{print $1}')
docker rmi -f $(docker images -a | grep 'ch-client' | awk '{print $3}')
docker rmi -f $(docker images -a | grep 'ch-server' | awk '{print $3}')
